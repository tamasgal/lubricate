[run]
source = {{: Package name :}}

[report]
include =
    {{: Package name :}}/*
omit =
    */__version__.py
    */tests/*
    */*/tests/*
    tests/*
exclude_lines =
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if self.debug:
    if settings.DEBUG
